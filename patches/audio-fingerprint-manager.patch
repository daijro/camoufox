+  if (seed == 0) {
+    aRv.ThrowTypeError("Audio fingerprint seed must be a non-zero integer");
+    return;
+  }
+  // Get userContextId from this window's document principal
+  uint32_t userContextId = 0;
+  Document* doc = GetExtantDoc();
+  if (doc) {
